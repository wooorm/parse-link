{
  "name": "parse-link",
  "version": "1.0.0",
  "description": "Unified URL parsing API in the browser and node",
  "license": "MIT",
  "keywords": [
    "url",
    "uri",
    "link",
    "parse",
    "parsing"
  ],
  "repository": "https://github.com/wooorm/parse-link",
  "bugs": "https://github.com/wooorm/parse-link/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "main": "index.js",
  "browser": "browser.js",
  "files": [
    "browser",
    "index.js"
  ],
  "dependencies": {
    "component-url": "^0.2.1"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify browser.js --bare -s parseLink > parse-link.js",
    "build-mangle": "esmangle parse-link.js > parse-link.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignores": [
      "parse-link.js"
    ]
  },
  "remarkConfig": {
    "plugins": ["preset-wooorm"]
  }
}
